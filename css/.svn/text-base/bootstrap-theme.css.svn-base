@media screen and (min-width: 600px) {

  div#pageslide {
    display: none !important;
    }
  .navbar-brand {
    float: left;
    margin-right: 5px;
    margin-left: -15px;
  }
  .navbar-nav {
    float: left;
    margin-top: 0;
    margin-bottom: 0;
  }
  .navbar-nav > li {
    float: left;
  }
  .navbar-nav > li > a {
    border-radius: 0;
  }
  .navbar-nav.pull-right {
    float: right;
    width: auto;
  }
  .navbar-toggle {
    position: relative;
    top: auto;
    left: auto;
    display: none;
  }
  .nav-collapse.collapse {
    display: block !important;
    height: auto !important;
    overflow: visible !important;
  }
  .navbar-collapse.collapse {
    display: none !important;
    height: auto !important;
    overflow: visible !important;
    padding-bottom: 0;
}

}
/*600 brakepoint mod*/
@media screen and (max-width: 599px) {
  .c-controls {
    display: none;
  }
  .carousel-inner {
    top: 0;
  }
  .container.carousel-inner {
    padding: 0;
    top: 0;
}
.helperhide {display:none;}
.navbar.navbar-default.navbar-fixed-top span {
    background-color: #f6f6f6;
    border-top: 1px solid #f6f6f6;
    display: none;
    font-size: 1.214em;
    height: 50px;
    padding-top: 12px;
    text-align: center;
}

}  
@media only screen and (min-device-width : 321px) and (max-device-width : 568px) and (orientation : landscape) {

.navbar-collapse {
    border-top: 1px solid transparent;
    box-shadow: 0 1px 0 rgba(255, 255, 255, 0.1) inset;
    /*max-height: 340px;*/
    max-height: 230px;
    overflow-x: visible;
    padding-left: 0;
    padding-right: 0;
}

}






